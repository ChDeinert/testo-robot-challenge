{
  "name": "testo-robot-challenge",
  "version": "1.0.0",
  "description": "Small coding challenge to show some code",
  "main": "index.js",
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "develop": "gatsby develop",
    "lint": "eslint . --ext .js,.ts",
    "start": "npm run build && gatsby serve",
    "test": "jest --bail",
    "test:watch": "jest --watch",
    "test:e2e": "start-server-and-test develop http://localhost:8000 cy:open",
    "test:e2e:ci": "start-server-and-test develop http://localhost:8000 cy:run",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChDeinert/testo-robot-challenge.git"
  },
  "author": "Christian Deinert <ChDeinert@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ChDeinert/testo-robot-challenge/issues"
  },
  "homepage": "https://github.com/ChDeinert/testo-robot-challenge#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@testing-library/cypress": "^7.0.6",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@types/jest": "^26.0.24",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "cypress": "^7.7.0",
    "eslint": "^7.30.0",
    "eslint-plugin-cypress": "^2.11.3",
    "gatsby-plugin-react-svg": "^3.0.1",
    "gh-pages": "^3.2.3",
    "jest": "^27.0.6",
    "start-server-and-test": "^1.12.6",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "gatsby": "^3.9.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.10.1"
  }
}
